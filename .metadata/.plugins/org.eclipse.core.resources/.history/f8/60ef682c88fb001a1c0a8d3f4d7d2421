
public class ProducerConsumer {

	public static void main(String[] args) {
		
	}

}
class Producer implements Runnable {
	public int[] queue;
	public Producer(int[] queue) {
		this.queue = queue;
	}
	
	public void run() {
		while (true) {
			
			Thread.yield();
		}
	}

}

class Consumer implements Runnable {
	public int[] queue;
	public Consumer(int[] queue) {
		this.queue = queue;
	}
	public void run() {
		
		while (true) {
			
		}
	}
}